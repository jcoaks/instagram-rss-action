{
  "name": "instagram-rss-action",
  "version": "0.2.0",
  "description": "A GitHub action that generates an RSS feed from a list of public Instagram accounts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/index.js && node src/docs.mjs",
    "prepare": "husky install"
  },
  "author": "Katy DeCorah",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@babel/eslint-parser": "^7.16.5",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.8",
    "emoji-regex": "^10.0.0",
    "instagram-web-api": "^2.2.2"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "extends": "eslint:recommended",
    "plugins": [
      "node",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false
    }
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/instagram-rss-action.git"
  },
  "keywords": [
    "github-actions"
  ],
  "bugs": {
    "url": "https://github.com/katydecorah/instagram-rss-action/issues"
  },
  "homepage": "https://github.com/katydecorah/instagram-rss-action#readme"
}
