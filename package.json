{
  "name": "instagram-rss-action",
  "version": "0.2.0",
  "description": "A GitHub action that generates an RSS feed from a list of public Instagram accounts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/index.js"
  },
  "author": "Katy DeCorah",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "dependencies": {
    "emoji-regex": "^9.2.1",
    "instagram-web-api": "^2.2.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "plugins": [
      "node",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/instagram-rss-action.git"
  },
  "keywords": [
    "github-actions"
  ],
  "bugs": {
    "url": "https://github.com/katydecorah/instagram-rss-action/issues"
  },
  "homepage": "https://github.com/katydecorah/instagram-rss-action#readme"
}
